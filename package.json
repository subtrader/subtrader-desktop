{
  "name": "subtrader-desktop",
  "productName": "SubTrader",
  "version": "1.0.0",
  "main": "main/index.js",
  "scripts": {
    "start": "electron .",
    "build": "next build renderer && next export renderer",
    "dist": "npm run build && build --dir"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "all",
    "singleQuote": true
  },
  "build": {
    "asar": false,
    "extraResources": [
      {
        "from": "renderer/out",
        "to": "app/renderer"
      }
    ]
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "binance-api-node": "^0.8.1",
    "electron": "^2.0.0",
    "electron-builder": "^20.13.4",
    "next": "^6.0.3",
    "node-binance-api": "^0.7.4",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "styled-components": "^3.3.3",
    "immer": "^1.3.1"
  },
  "dependencies": {
    "dotenv": "^6.0.0",
    "electron-is-dev": "^0.3.0",
    "electron-next": "^3.1.4",
    "electron-store": "^2.0.0"
  }
}
